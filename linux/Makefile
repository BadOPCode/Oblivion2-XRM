.PHONY: clean All

All:
	@echo "----------Building project:[ xrm-menu-convert - Debug ]----------"
	@cd "/home/merc/code/Oblivion2-XRM/xrm-menu-convert" && "$(MAKE)" -f  "xrm-menu-convert.mk"
clean:
	@echo "----------Cleaning project:[ xrm-menu-convert - Debug ]----------"
	@cd "/home/merc/code/Oblivion2-XRM/xrm-menu-convert" && "$(MAKE)" -f  "xrm-menu-convert.mk" clean
