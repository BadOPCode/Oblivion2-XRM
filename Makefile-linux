.PHONY: clean All

All:
	@echo "----------Building project:[ xrm-unittest (LINUX) - Debug ]----------"
	@cd "sqlitewrap" && $(MAKE) -f  "Makefile"
	@$(MAKE) -f  "xrm-server.mk-linux"
	@cd "xrm-unittest" && $(MAKE) -f  "xrm-unittest.mk-linux"
clean:
	@echo "----------Cleaning project:[ xrm-unittest (LINUX) - Debug ]----------"
	@cd "sqlitewrap" && $(MAKE) -f  "SqliteWrapped.mk-linux" clean
	@$(MAKE) -f  "xrm-server.mk-linux" clean
	@cd "xrm-unittest" && $(MAKE) -f  "xrm-unittest.mk-linux" clean

